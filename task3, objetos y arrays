let edad = true + false;
let numero = 100 + 500;
let binombel = 500n + 200n;
let letra = "Ho" + "la";
let indef = undefined + undefined;

console.log(`${edad} [${typeof edad}]`);  
console.log(`${numero} [${typeof numero}]`);
console.log(`${binombel} [${typeof binombel}]`);
console.log(`${letra} [${typeof letra}]`);
console.log(`${indef} [${typeof indef}]`);

