const string = "Caroline";
const number = 160;
const bigint1 = 1n;
const bigint2 = bigint1 + 2n;
const isBooleanReal = true;
let indef;

let oracion = `${typeof string} : ${string}, ${typeof number} : ${number},
${typeof bigint1} : ${bigint1}, ${typeof isBooleanReal} : ${isBooleanReal}, 
${typeof indef} : ${indef}.`;

console.log(oracion);

console.log(typeof string);
console.log(typeof number);
console.log(typeof bigint1);
console.log(typeof bigint2);
