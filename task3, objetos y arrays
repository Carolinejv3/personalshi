
let string1 = String ("Caroline");
let string2 = String ("Jimenez");
let number1 = Number (17);
let number2 = Number (18);
let bigint1 = BigInt (1n);
let bigint2 = BigInt (bigint1 + 2n);
let isBooleanReal = Boolean(true);
let isCarolineEighteen = Boolean(false);
let undef1 = undefined;


let oracion = `${typeof string1} : ${string1}, ${typeof string2} : ${string2}, ${typeof number1} : ${number1}, 
${typeof number2} : ${number2}, ${typeof bigint1} : ${bigint1}, ${typeof bigint2} : ${bigint2}, 
${typeof isBooleanReal} : ${isBooleanReal}, ${typeof isCarolineEighteen} : ${isCarolineEighteen},
${typeof indef} : ${undef}.`;

console.log(oracion);


console.log(`${string1} [${typeof string1}]`);
console.log(`${string2} [${typeof string2}]`);
console.log(`${number1} [${typeof number1}]`);
console.log(`${number2} [${typeof number2}]`);
console.log(`${bigint1} [${typeof bigint1}]`);
console.log(`${bigint2} [${typeof bigint2}]`);
console.log(`${isBooleanReal} [${typeof isBooleanReal}]`);
console.log(`${isCarolineEighteen} [${typeof isCarolineEighteen}]`);
console.log(`${undef1} [${typeof undef1}]`);




